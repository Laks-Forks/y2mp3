{
  "dependencies": {
    "downloads-folder": "^1.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "youtube-mp3-downloader": "git+https://github.com/moshfeu/youtube-mp3-downloader.git",
    "youtube-playlist": "^1.0.1",
    "electron-is-dev": "^1.0.1"
  },
  "name": "download-youtube-playlist",
  "version": "1.0.0",
  "main": "main.js",
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.8",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "css-loader": "^1.0.1",
    "electron": "^3.0.8",
    "electron-builder": "^20.28.4",
    "ffbinaries": "^1.1.0",
    "file-loader": "^2.0.0",
    "node-sass": "^4.10.0",
    "object-assign": "^4.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "ytdl-core": "^0.26.3"
  },
  "scripts": {
    "webpack": "webpack --watch",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-windows": "npm run dist -- --win"
  },
  "author": "MosheF",
  "license": "MIT",
  "description": "An app to download youtube playlists and onvert to mp3",
  "build": {
    "appId": "moshfeu.youtube.playlist.downloader",
    "files": [
      "index.html",
      "main.js",
      "resources/**"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  }
}
